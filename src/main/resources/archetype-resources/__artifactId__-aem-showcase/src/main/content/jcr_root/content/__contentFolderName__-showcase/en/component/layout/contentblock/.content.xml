#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[${contentFolderName}-showcase:content-type/page]"
        cq:template="/conf/${confFolderName}-showcase/settings/wcm/templates/twocolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Content Block"
        sling:resourceType="${appsFolderName}/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="${appsFolderName}/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/layout/container">
                <page-details
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/details/page-details"
                    showBreadcrumb="false"
                    showToolbar="false">
                </page-details>
                <contentblockmenu1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblockmenu"
                        componentId="contentblockmenu1"/>
                <contentblock1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentId="contentblock1"
                    componentTheme="[]"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="Variant: Default">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            text="&lt;p>&lt;b>Variant&lt;/b>: Default&lt;/p>&${symbol_pound}xa;"
                            textIsRich="true"/>
                    </par>
                </contentblock1>
                <contentblock2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentId="contentblock2"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="Variant: Default Container"
                    variant="container">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            text="&lt;p>&lt;b>Variant&lt;/b>: Default Container&lt;/p>&${symbol_pound}xa;"
                            textIsRich="true"/>
                    </par>
                </contentblock2>
                <contentblock3
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentId="contentblock3"
                    hideTitle="false"
                    hideTitleSeparator="false"
                    title="Variant: Float Section"
                    variant="floating">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            text="&lt;p>&lt;b>Variant&lt;/b>: Float Section&lt;br>&${symbol_pound}xa;&lt;/p>&${symbol_pound}xa;"
                            textIsRich="true"/>
                    </par>
                </contentblock3>

                <contentblock4
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentId="contentblock4"
                    hideTitle="false"
                    hideTitleSeparator="false"
                    title="Variant: Description List Section"
                    variant="descriptionlist">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            text="&lt;p>&lt;b>Variant&lt;/b>: Description List Section&lt;br>&${symbol_pound}xa;&lt;/p>&${symbol_pound}xa;"
                            textIsRich="true"/>
                    </par>
                </contentblock4>

                <contentblock5
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentId="contentblock5"
                    hideTitle="false"
                    hideTitleSeparator="false"
                    title="Variant: Field Set Section"
                    variant="fieldset">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            text="&lt;p>&lt;b>Variant&lt;/b>: Field Set Section&lt;br>&${symbol_pound}xa;&lt;/p>&${symbol_pound}xa;"
                            textIsRich="true"/>
                    </par>
                </contentblock5>


                <contentblock6
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentId="contentblock6"
                    hideInMenu="false"
                    hideTitle="true"
                    hideTitleSeparator="false"
                    hideTopLink="true"
                    title="Variant: Advanced Plain Section"
                    titleType="h2"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            text="&lt;p>&lt;b>Variant&lt;/b>: Advanced Plain Section&lt;/p>&${symbol_pound}xa;"
                            textIsRich="true"/>
                    </par>
                </contentblock6>
                <contentblock7
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock7"
                        hideInMenu="false"
                        hideTitle="false"
                        hideTitleSeparator="false"
                        linksLeft="/content/${contentFolderName}-showcase/en/component/layout/contentblock"
                        linksLeftTitle="Left Links"
                        linksRight="/content/${contentFolderName}-showcase/en/component/layout/contentblock"
                        linksRightTitle="Right Links"
                        title="Variant: Advanced Section with Links"
                        titleType="p"
                        variant="advsection">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/content/text"
                                text="&lt;p>&lt;b>Variant&lt;/b>: Advanced Section with Links&lt;/p>&${symbol_pound}xa;"
                                textIsRich="true"/>
                    </par>
                </contentblock7>

                <contentblock8
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock8"
                        componentTheme="[]"
                        hideTitle="false"
                        hideTitleSeparator="true"
                        imageRotate="0"
                        title="Variant: Section with Background">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/content/text"
                                text="&lt;p>&lt;b>Variant&lt;/b>: Section with Background&lt;/p>&${symbol_pound}xa;"
                                textIsRich="true"/>
                    </par>
                    <bgimage
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/media/image"
                            fileReference="/content/dam/${contentFolderName}-showcase/en/common/aem-design.png"/>
                </contentblock8>

                <contentblock9
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock9"
                        componentTheme="[]"
                        hideTitle="false"
                        hideTitleSeparator="true"
                        variant="containerVideo"
                        title="Variant: Section with Background Video">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/content/text"
                                text="&lt;p>&lt;b>Variant&lt;/b>: Section with Background Video&lt;/p>&${symbol_pound}xa;"
                                textIsRich="true"/>
                    </par>
                    <bgimage
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/media/image"
                            fileReference="/content/dam/${contentFolderName}-showcase/en/common/aem-design.png"/>
                    <bgvideo
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/media/image"
                            fileReference="/content/dam/${contentFolderName}-showcase/en/multimedia/video/Clouds.mov"/>
                </contentblock9>

                <contentblock10
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock10"
                        componentTheme="[]"
                        hideTitle="false"
                        hideTitleSeparator="true"
                        variant="parsys"
                        title="Variant: Parsys">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/parsys">
                        <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/content/text"
                                text="&lt;p>&lt;b>Variant&lt;/b>: Parsys&lt;/p>&${symbol_pound}xa;"
                                textIsRich="true"/>
                    </par>
                </contentblock10>


                <contentblock11
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock11"
                        componentTheme="[]"
                        hideTitle="false"
                        hideTitleSeparator="true"
                        variant="advsection"
                        title="Variant: Child Section">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/parsys">
                        <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/content/text"
                                text="&lt;p>&lt;b>Variant&lt;/b>: Child Section used for collapsible sections &lt;/p>&${symbol_pound}xa;"
                                textIsRich="true"/>
                        <contentblock11a
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/layout/contentblock"
                                componentId="contentblock11a"
                                componentTheme="[]"
                                hideTitle="false"
                                hideTitleSeparator="true"
                                variant="childsection"
                                dataParent="contentblock11"
                                dataToggle="collapse"
                                title="Collapsible Group Item ${symbol_pound}1">
                            <par
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="${appsFolderName}/components/layout/parsys">
                                <text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="${appsFolderName}/components/content/text"
                                        text="&lt;p>&lt;b>Variant&lt;/b>: Child Section used for collapsible sections &lt;/p>&${symbol_pound}xa;"
                                        textIsRich="true"/>
                            </par>
                        </contentblock11a>
                        <contentblock11b
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/layout/contentblock"
                                componentId="contentblock11b"
                                componentTheme="[]"
                                hideTitle="false"
                                hideTitleSeparator="true"
                                variant="childsection"
                                dataParent="contentblock11"
                                dataToggle="collapse"
                                title="Collapsible Group Item ${symbol_pound}2">
                            <par
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="${appsFolderName}/components/layout/parsys">
                                <text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="${appsFolderName}/components/content/text"
                                        text="&lt;p>&lt;b>Variant&lt;/b>: Child Section used for collapsible sections &lt;/p>&${symbol_pound}xa;"
                                        textIsRich="true"/>
                            </par>
                        </contentblock11b>
                        <contentblock11c
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/layout/contentblock"
                                componentId="contentblock11c"
                                componentTheme="[]"
                                hideTitle="false"
                                hideTitleSeparator="true"
                                variant="childsection"
                                dataParent="contentblock11"
                                dataToggle="collapse"
                                title="Collapsible Group Item ${symbol_pound}3">
                            <par
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="${appsFolderName}/components/layout/parsys">
                                <text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="${appsFolderName}/components/content/text"
                                        text="&lt;p>&lt;b>Variant&lt;/b>: Child Section used for collapsible sections &lt;/p>&${symbol_pound}xa;"
                                        textIsRich="true"/>
                            </par>
                        </contentblock11c>
                    </par>
                </contentblock11>

                <contentblock12
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock12"
                        componentTheme="[]"
                        componentModifiers="[${contentFolderName}-showcase:component-style-modifier/height/banner]"
                        hideTitle="false"
                        hideTitleSeparator="true"
                        imageRotate="0"
                        title="Variant: Section with Background using Image Option - Manual MediaQuery with Rendition">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/content/text"
                                text="&lt;p>&lt;b>Variant&lt;/b>: Section with Background using Image Option - Manual MediaQuery with Rendition&lt;/p>"
                                textIsRich="true"/>
                    </par>
                    <bgimage
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/media/image"
                            assetMediaQuery="[${contentFolderName}-showcase:component-dialog/css/mediaquery/medium,${contentFolderName}-showcase:component-dialog/css/mediaquery/landscape-large,${contentFolderName}-showcase:component-dialog/css/mediaquery/extra-large]"
                            assetMediaQueryRendition="[cq5dam.web.1280.1280.png,aem-design-logo.png,cq5dam.web.1280.1280.png]"
                            fileReference="/content/dam/${contentFolderName}-showcase/en/common/aem-design.png"
                            imageOption="mediaqueryrendition"/>
                </contentblock12>
            </par>
        </article>
    </jcr:content>
</jcr:root>
