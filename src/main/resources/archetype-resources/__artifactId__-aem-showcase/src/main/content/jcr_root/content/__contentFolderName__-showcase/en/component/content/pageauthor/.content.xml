#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:lastModifiedBy="admin"
        cq:template="/conf/${confFolderName}-showcase/settings/wcm/templates/twocolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Page Author"
        sling:resourceType="${appsFolderName}/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="${appsFolderName}/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/layout/container">
                <contentblockmenu1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblockmenu"
                        componentId="contentblockmenu1"/>
                <contentblock1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock1"
                        componentTheme="[]"
                        hideTitle="false"
                        hideTitleSeparator="true"
                        title="Page Author: Default"
                        variant="advsection">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <pageauthor
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/content/pageauthor"
                                componentId="pageauthor1"
                                textIsRich="true"/>
                    </par>
                </contentblock1>
                <contentblock2
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock2"
                        componentTheme="[]"
                        hideTitle="false"
                        hideTitleSeparator="true"
                        title="Page Author: Simple"
                        variant="advsection">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <pageauthor
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/content/pageauthor"
                                componentId="pageauthor2"
                                variant="simple"
                                textIsRich="true"/>
                    </par>
                </contentblock2>
                <contentblock3
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock3"
                        componentTheme="[]"
                        hideTitle="false"
                        hideTitleSeparator="true"
                        title="Page Author: Default with Override"
                        variant="advsection">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <pageauthor
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/content/pageauthor"
                                componentId="pageauthor3"
                                author="Author Full Name"
                                authorUrl="http://aem.design"
                                textIsRich="true"/>
                    </par>
                </contentblock3>
                <contentblock4
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock4"
                        componentTheme="[]"
                        hideTitle="false"
                        hideTitleSeparator="true"
                        title="Page Author: Simple with Override"
                        variant="advsection">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <pageauthor
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/content/pageauthor"
                                componentId="pageauthor4"
                                author="Author Full Name"
                                authorUrl="http://aem.design"
                                variant="simple"
                                textIsRich="true"/>
                    </par>
                </contentblock4>
            </par>
        </article>
    </jcr:content>
</jcr:root>
