#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
       cq:tags="[${contentFolderName}-showcase:content-type/page/event]"
        cq:template="/conf/${confFolderName}-showcase/settings/wcm/templates/onecolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Event Details 1"
        sling:resourceType="${appsFolderName}/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="${appsFolderName}/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/layout/container">
                <event-details
                   jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/details/event-details"
                   jcr:mixinTypes="[cq:Taggable]"
                    componentId="event-details"
                    description="Event Description"
                    eventDisplayDate="15.01.2016 to 03.02.2016"
                    eventEndDate="{Date}2016-02-03T00:00:00.000+08:00"
                    eventLoc="Connecting Space"
                    eventRefLabel="Event Label"
                    eventRefLink="/content/${contentFolderName}-showcase/en/component/lists/event-list"
                    eventStartDate="{Date}2016-01-15T16:12:00.000+08:00"
                    eventStartTime="12:00 PM"
                    metaDataSep="Event"
                    title="Event Title">
                    <secondaryImage
                            jcr:primaryType="nt:unstructured"
                            fileReference="/content/dam/${contentFolderName}-showcase/en/components/media/image/city1.jpg"
                            imageRotate="0"/>
                </event-details>
            </par>
        </article>
        <image
            jcr:primaryType="nt:unstructured"
            fileReference="/content/dam/${contentFolderName}-showcase/en/components/content/download/an-introduction-to-git-9-638.jpg"
            imageRotate="0"/>
        <header
            jcr:primaryType="nt:unstructured"
            sling:resourceType="${appsFolderName}/components/layout/header">
            <bgimage
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/media/image"
                imageRotate="0"/>
            <logo
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/media/image"
                imageRotate="0"/>
        </header>
    </jcr:content>
</jcr:root>
