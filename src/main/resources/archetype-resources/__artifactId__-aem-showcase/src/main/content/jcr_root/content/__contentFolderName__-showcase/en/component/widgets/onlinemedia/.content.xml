#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:tags="[${contentFolderName}-showcase:content-type/page]"
            cq:template="/conf/${confFolderName}-showcase/settings/wcm/templates/twocolumn"
            jcr:primaryType="cq:PageContent"
            jcr:title="Online Media"
            sling:resourceType="${appsFolderName}/components/template/base">
        <article
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/layout/container">
                <page-details
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/details/page-details"/>

                <contentblock1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock1"
                        hideTitle="false"
                        hideTitleSeparator="true"
                        title="Online Media: Default Variant - Default No Content"
                        variant="advsection">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <onlinemedia
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/widgets/onlinemedia"
                                componentId="onlinemedia1"
                                mediaProvider="default"
                                variant="default"/>
                    </par>
                </contentblock1>
                <contentblock2
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock2"
                        hideTitle="false"
                        hideTitleSeparator="true"
                        title="Online Media: Default Variant - YouTube Provider"
                        variant="advsection">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <onlinemedia
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/widgets/onlinemedia"
                                componentId="onlinemedia2"
                                componentModifiers="[${contentFolderName}-showcase:component-style-modifier/online-media/responsive]"
                                mediaId="tL46xeIV5mc"
                                mediaProvider="youtube"
                                variant="default"/>
                    </par>
                </contentblock2>
                <contentblock3
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock3"
                        hideTitle="false"
                        title="Online Media: Default Variant - Kaltura Provider"
                        variant="advsection">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <onlinemedia
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/widgets/onlinemedia"
                                componentHeight="850"
                                componentId="onlinemedia3"
                                componentModifiers="[${contentFolderName}-showcase:component-style-modifier/online-media/responsive]"
                                componentWidth="1000"
                                mediaId="1_lph7zzb1"
                                mediaPartnerId="_691292"
                                mediaPlayerId="20499062"
                                mediaProvider="kaltura"
                                mediaTitle="kaltura video"
                                variant="default"/>
                    </par>
                </contentblock3>
                <contentblock4
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock4"
                        hideTitle="false"
                        title="Online Media: Iframe Variant - Youtube"
                        variant="advsection">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <onlinemedia
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/widgets/onlinemedia"
                                componentBooleanAttrs="[${contentFolderName}-showcase:component-style-modifier/schema/widgets/onlinemedia/video,${contentFolderName}-showcase:component-style-modifier/video/allowfullscreen,${contentFolderName}-showcase:component-style-modifier/video/webkitallowfullscreen,${contentFolderName}-showcase:component-style-modifier/video/mozAllowFullScreen]"
                                componentHeight="500"
                                componentId="onlinemedia4"
                                componentModifiers="[${contentFolderName}-showcase:component-style-modifier/embed/embed-responsive-4by3]"
                                componentWidth="950"
                                mediaProviderUrl="https://www.youtube.com/embed/Dk7h22mRYHQ"
                                variant="iframe"/>
                    </par>
                </contentblock4>
                <contentblock5
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock5"
                        hideTitle="false"
                        title="Online Media: Iframe Variant - Vimeo"
                        variant="advsection">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <onlinemedia
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/widgets/onlinemedia"
                                componentId="onlinemedia5"
                                componentModifiers="[${contentFolderName}-showcase:component-style-modifier/embed/embed-responsive-4by3]"
                                mediaProviderUrl="https://player.vimeo.com/video/8733915"
                                variant="iframe"/>
                    </par>
                </contentblock5>
                <contentblock6
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock6"
                        hideTitle="false"
                        title="Online Media: Iframe Variant - issuu"
                        variant="advsection">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <onlinemedia
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/widgets/onlinemedia"
                                componentId="onlinemedia6"
                                componentModifiers="[${contentFolderName}-showcase:component-style-modifier/embed/embed-responsive-4by3]"
                                mediaProviderUrl="https://e.issuu.com/anonymous-embed.html?u=issuu&amp;amp;d=issuu_digitaltoolkit"
                                variant="iframe"/>
                    </par>
                </contentblock6>
            </par>
        </article>
    </jcr:content>
</jcr:root>
