<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:allowedTemplates="[/conf/${confFolderName}-showcase/settings/wcm/templates/.*]"
        cq:template="/conf/${confFolderName}-showcase/settings/wcm/templates/onecolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Hidden"
        sling:resourceType="${appsFolderName}/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="${appsFolderName}/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/layout/container">
                <contentblock1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentId="contentblock1"
                    title="Default"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <hidden
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/forms/hidden"
                            id="hidden1"
                            name="hidden1"
                            value="hidden"/>
                    </par>
                </contentblock1>
            </par>
        </article>
    </jcr:content>
</jcr:root>
