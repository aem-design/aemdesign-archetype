<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:allowedTemplates="[/conf/${confFolderName}-showcase/settings/wcm/templates/.*]"
        cq:template="/conf/${confFolderName}-showcase/settings/wcm/templates/onecolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Options"
        sling:resourceType="${appsFolderName}/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="${appsFolderName}/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/layout/container">
                <contentblock1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentId="contentblock1"
                    title="Default"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <options
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Checkboxes"
                            sling:resourceType="${appsFolderName}/components/forms/options"
                            name="options1"
                            source="local"
                            type="checkbox">
                            <items jcr:primaryType="nt:unstructured">
                                <item0
                                    jcr:primaryType="nt:unstructured"
                                    selected="false"
                                    text="Yes"
                                    value="yes"/>
                                <item1
                                    jcr:primaryType="nt:unstructured"
                                    selected="false"
                                    text="No"
                                    value="no"/>
                            </items>
                        </options>
                    </par>
                </contentblock1>
                <contentblock2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentId="contentblock2"
                    title="Radio Buttons"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <options
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Radio buttons"
                            sling:resourceType="${appsFolderName}/components/forms/options"
                            name="options2"
                            source="local"
                            type="radio">
                            <items jcr:primaryType="nt:unstructured">
                                <item0
                                    jcr:primaryType="nt:unstructured"
                                    selected="false"
                                    text="Yes"
                                    value="yes"/>
                                <item1
                                    jcr:primaryType="nt:unstructured"
                                    selected="false"
                                    text="No"
                                    value="no"/>
                            </items>
                        </options>
                    </par>
                </contentblock2>
                <contentblock3
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentId="contentblock3"
                    title="Dropdown"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <options
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Dropdown"
                            sling:resourceType="${appsFolderName}/components/forms/options"
                            name="options3"
                            source="local"
                            type="drop-down">
                            <items jcr:primaryType="nt:unstructured">
                                <item0
                                    jcr:primaryType="nt:unstructured"
                                    selected="false"
                                    text="Yes"
                                    value="yes"/>
                                <item1
                                    jcr:primaryType="nt:unstructured"
                                    selected="false"
                                    text="No"
                                    value="no"/>
                            </items>
                        </options>
                    </par>
                </contentblock3>
                <contentblock4
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentId="contentblock4"
                    title="Mutli Select Dropdown"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <options
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Multi Select Dropdown"
                            sling:resourceType="${appsFolderName}/components/forms/options"
                            name="options4"
                            source="local"
                            type="multi-drop-down">
                            <items jcr:primaryType="nt:unstructured">
                                <item0
                                    jcr:primaryType="nt:unstructured"
                                    selected="false"
                                    text="Yes"
                                    value="yes"/>
                                <item1
                                    jcr:primaryType="nt:unstructured"
                                    selected="false"
                                    text="No"
                                    value="no"/>
                            </items>
                        </options>
                    </par>
                </contentblock4>
            </par>
        </article>
    </jcr:content>
</jcr:root>
