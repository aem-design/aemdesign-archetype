#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:tags="[${contentFolderName}-showcase:content-type/page]"
            cq:template="/conf/${confFolderName}-showcase/settings/wcm/templates/twocolumn"
            jcr:primaryType="cq:PageContent"
            jcr:title="List"
            sling:resourceType="${appsFolderName}/components/template/base">
        <article
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/layout/article">
            <par
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/container">
                <page-details
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/details/page-details"/>
                <contentblock1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        title="List: Default variant using Default badge"
                        variant="advsection"
                        componentId="contentblock1"
                        hideTitle="false"
                        hideTitleSeparator="true">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/content/text"
                                text=""
                                textIsRich="true"
                                variant="simple"/>
                        <list
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/lists/list"
                                componentId="list1"
                                listFrom="children"
                                listType="list"
                                parentPage="/content/${contentFolderName}-showcase/en/component/lists/list/images"/>
                    </par>
                </contentblock1>


                <contentblock2
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        title="List: Default variant using Icon badge"
                        variant="advsection"
                        componentId="contentblock2"
                        hideTitle="false"
                        hideTitleSeparator="true">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/content/text"
                                text=""
                                textIsRich="true"
                                variant="simple"/>
                        <list
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/lists/list"
                                componentId="list2"
                                detailsBadge="icon"
                                listFrom="children"
                                listType="list"
                                parentPage="/content/${contentFolderName}-showcase/en/component/lists/list/icons"/>
                    </par>
                </contentblock2>



                <contentblock3
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        title="List: Default variant using Image badge"
                        variant="advsection"
                        componentId="contentblock3"
                        hideTitle="false"
                        hideTitleSeparator="true">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/content/text"
                                text=""
                                textIsRich="true"
                                variant="simple"/>
                        <list
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/lists/list"
                                componentId="list3"
                                detailsBadge="image"
                                listFrom="children"
                                listType="list"
                                parentPage="/content/${contentFolderName}-showcase/en/component/lists/list/images"/>
                    </par>
                </contentblock3>



                <contentblock4
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        title="List: Default variant using Card badge"
                        variant="advsection"
                        componentId="contentblock4"
                        hideTitle="false"
                        hideTitleSeparator="true">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/content/text"
                                text=""
                                textIsRich="true"
                                variant="simple"/>
                        <list
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/lists/list"
                                componentId="list4"
                                detailsBadge="cardImageTitleCategoryDescription"
                                listFrom="children"
                                listType="list"
                                parentPage="/content/${contentFolderName}-showcase/en/component/lists/list/images"/>
                    </par>
                </contentblock4>


                <contentblock5
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        title="List: Default variant using Card Icon badge"
                        variant="advsection"
                        componentId="contentblock5"
                        hideTitle="false"
                        hideTitleSeparator="true">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/content/text"
                                text=""
                                textIsRich="true"
                                variant="simple"/>
                        <list
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/lists/list"
                                componentId="list5"
                                detailsBadge="cardIcon"
                                listFrom="children"
                                listType="list"
                                parentPage="/content/${contentFolderName}-showcase/en/content/icons"/>
                    </par>
                </contentblock5>



                <contentblock6
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        title="List: Default Empty"
                        variant="advsection"
                        componentId="contentblock6"
                        hideTitle="false"
                        hideTitleSeparator="true">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/content/text"
                                text=""
                                textIsRich="true"
                                variant="simple"/>
                        <list
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/lists/list"
                                componentId="list6"/>
                    </par>
                </contentblock6>





            </par>
        </article>
    </jcr:content>
</jcr:root>
