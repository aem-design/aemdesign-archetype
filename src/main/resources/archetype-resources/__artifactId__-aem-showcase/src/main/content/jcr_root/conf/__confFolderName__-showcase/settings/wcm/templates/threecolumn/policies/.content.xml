#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:policy="${confFolderName}/components/template/base/policy_default"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="wcm/core/components/policies/mappings">
        <header jcr:primaryType="nt:unstructured">
            <par
                    cq:policy="wcm/foundation/components/responsivegrid/${confFolderName}-default"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/core/components/policies/mapping">
                <${confFolderName} jcr:primaryType="nt:unstructured">
                    <components jcr:primaryType="nt:unstructured">
                        <media jcr:primaryType="nt:unstructured">
                            <image
                                    cq:policy="${confFolderName}/components/media/image/policy_default_image"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="wcm/core/components/policies/mapping"/>
                        </media>
                    </components>
                </${confFolderName}>
            </par>
        </header>
        <aside jcr:primaryType="nt:unstructured">
            <par
                    cq:policy="wcm/foundation/components/responsivegrid/${confFolderName}-default"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/core/components/policies/mapping">
                <${confFolderName} jcr:primaryType="nt:unstructured">
                    <components jcr:primaryType="nt:unstructured">
                        <media jcr:primaryType="nt:unstructured">
                            <image
                                    cq:policy="${confFolderName}/components/media/image/policy_default_image"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="wcm/core/components/policies/mapping"/>
                        </media>
                    </components>
                </${confFolderName}>
            </par>
        </aside>
        <article jcr:primaryType="nt:unstructured">
            <par
                    cq:policy="wcm/foundation/components/responsivegrid/${confFolderName}-default"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/core/components/policies/mapping">
                <${confFolderName} jcr:primaryType="nt:unstructured">
                    <components jcr:primaryType="nt:unstructured">
                        <media jcr:primaryType="nt:unstructured">
                            <image
                                    cq:policy="${confFolderName}/components/media/image/policy_default_image"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="wcm/core/components/policies/mapping"/>
                        </media>
                    </components>
                </${confFolderName}>
            </par>
        </article>
        <asideright jcr:primaryType="nt:unstructured">
            <par
                    cq:policy="wcm/foundation/components/responsivegrid/${confFolderName}-default"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/core/components/policies/mapping">
                <${confFolderName} jcr:primaryType="nt:unstructured">
                    <components jcr:primaryType="nt:unstructured">
                        <media jcr:primaryType="nt:unstructured">
                            <image
                                    cq:policy="${confFolderName}/components/media/image/policy_default_image"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="wcm/core/components/policies/mapping"/>
                        </media>
                    </components>
                </${confFolderName}>
            </par>
        </asideright>
        <footer jcr:primaryType="nt:unstructured">
            <par
                    cq:policy="wcm/foundation/components/responsivegrid/${confFolderName}-default"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/core/components/policies/mapping">
                <${confFolderName} jcr:primaryType="nt:unstructured">
                    <components jcr:primaryType="nt:unstructured">
                        <media jcr:primaryType="nt:unstructured">
                            <image
                                    cq:policy="${confFolderName}/components/media/image/policy_default_image"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="wcm/core/components/policies/mapping"/>
                        </media>
                    </components>
                </${confFolderName}>
            </par>
        </footer>

    </jcr:content>
</jcr:root>
