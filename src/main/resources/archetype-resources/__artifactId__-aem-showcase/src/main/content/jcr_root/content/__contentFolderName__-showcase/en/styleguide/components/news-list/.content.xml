#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/${confFolderName}-showcase/settings/wcm/templates/styleguide"
        jcr:primaryType="cq:PageContent"
        jcr:title="News List"
        sling:resourceType="${appsFolderName}/components/template/styleguide">
        <aside
            jcr:primaryType="nt:unstructured"
            sling:resourceType="${appsFolderName}/components/layout/aside">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/layout/container"/>
        </aside>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="${appsFolderName}/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/layout/container">
                <page_details
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/details/page-details"
                    componentId="page_details"
                    componentTheme="[${contentFolderName}-showcase:component-style-theme/styleguide/page-details]"
                    editable="{Boolean}false"
                    showBreadcrumb="false"
                    showPageDate="false"
                    showParsys="false"
                    showToolbar="false"
                    textIsRich="true"/>
                <contentblock
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentId="contentblock-1"
                    componentModifiers="[${contentFolderName}-showcase:component-style-modifier/padding/p-15,${contentFolderName}-showcase:component-style-modifier/padding/p-md-30]">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <contentblock_1872776108
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/contentblock">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/layout/container">
                                <newslist
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="${appsFolderName}/components/lists/newslist"
                                    componentId="newslist-1"
                                    detailsBadge="cardImageTitleCategoryActionDate"
                                    listFrom="static"
                                    pages="[/content/${contentFolderName}-showcase/au/en/styleguide/components/news-details,/content/${contentFolderName}-showcase/au/en/styleguide/components/news-details,/content/${contentFolderName}-showcase/au/en/styleguide/components/news-details]"
                                    tagsMatch="any">
                                    <badgeThumbnail
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="${appsFolderName}/components/media/image"/>
                                </newslist>
                            </par>
                        </contentblock_1872776108>
                    </par>
                    <bgimage
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/media/image"/>
                    <bgvideo
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/media/image"/>
                </contentblock>
            </par>
        </article>
    </jcr:content>
</jcr:root>
