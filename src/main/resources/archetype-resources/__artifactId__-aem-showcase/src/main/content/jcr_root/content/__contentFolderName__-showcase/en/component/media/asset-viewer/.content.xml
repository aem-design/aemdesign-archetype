#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[]"
        cq:template="/conf/${confFolderName}/settings/wcm/templates/twocolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Asset Viewer"
        sling:resourceType="${appsFolderName}/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="/apps/${appsFolderName}/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/layout/container">
                <start
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/media/asset/editor/form/start"
                    actionType="foundation/components/form/actions/edit"
                    clientValidation="true"
                    formid="Asset_Form_Metadata"/>
                <contentblock_breadcrumb
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    hideTitle="true"
                    hideTitleSeparator="true">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <breadcrumb
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/breadcrumb"
                            componentId="breadcrumbs"/>
                        <toolbar
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/navbar"
                            cancelInheritParent="true">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/layout/container">
                                <listNav
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="${appsFolderName}/components/lists/listnav"
                                    backPage="/content/${contentFolderName}-showcase/en/component/media/media-gallery-assetviewer/"
                                    collection="/content/dam/collections/8/8fU0fvjsVeZXBq-11t1X/m_collection"
                                    listFrom="collection"/>
                                <sharebutton
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="${appsFolderName}/components/widgets/addthis/sharebutton"/>
                            </par>
                        </toolbar>
                    </par>
                </contentblock_breadcrumb>
                <contentblock_detail
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentTheme="[]"
                    hideTitle="true"
                    hideTitleSeparator="true">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <detail
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/media/asset/editor/detail"/>
                    </par>
                </contentblock_detail>
                <thumbnail
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/media/asset/editor/thumbnail"
                    showBreadcrumb="false"/>
                <contentblock_metadata
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentTheme="[]"
                    hideTitle="true"
                    hideTitleSeparator="true">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <contentblock_firstrow
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/contentblock"
                            hideTitle="true"
                            hideTitleSeparator="true"
                            variant="descriptionlist">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/layout/container">
                                <daterange
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="${appsFolderName}/components/media/asset/editor/form/dateRange"
                                    fromDateField="dam:startDate"
                                    title="Date"
                                    toDateField="dam:finishDate">
                                    <meta
                                        jcr:primaryType="nt:unstructured"
                                        namespace="dc"
                                        type="String"/>
                                </daterange>
                                <metadata
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceSuperType="foundation/components/form/defaults/field"
                                    sling:resourceType="${appsFolderName}/components/media/asset/editor/form/metadata"
                                    readOnly="true">
                                    <meta
                                        jcr:primaryType="nt:unstructured"
                                        label="Medium"
                                        localPart="medium"
                                        namespace="dam"
                                        type="Tags"/>
                                </metadata>
                            </par>
                        </contentblock_firstrow>
                        <contentblock_secondrow
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/contentblock"
                            hideTitle="true"
                            hideTitleSeparator="true"
                            variant="descriptionlist">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/layout/container">
                                <metadata
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceSuperType="foundation/components/form/defaults/field"
                                    sling:resourceType="${appsFolderName}/components/media/asset/editor/form/metadata"
                                    readOnly="true">
                                    <meta
                                        jcr:primaryType="nt:unstructured"
                                        label="Dimension"
                                        localPart="dimension"
                                        namespace="dam"
                                        type="String"/>
                                </metadata>
                                <metadata_1622703178
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceSuperType="foundation/components/form/defaults/field"
                                    sling:resourceType="${appsFolderName}/components/media/asset/editor/form/metadata"
                                    readOnly="true">
                                    <meta
                                        jcr:primaryType="nt:unstructured"
                                        label="Credit"
                                        localPart="credit"
                                        namespace="dam"
                                        type="String"/>
                                </metadata_1622703178>
                            </par>
                        </contentblock_secondrow>
                    </par>
                </contentblock_metadata>
                <contentblock-mobile
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentTheme="[]"
                    hideTitle="true"
                    hideTitleSeparator="true">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <toolbar
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/navbar"
                            cancelInheritParent="true">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/layout/container">
                                <listNav
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="${appsFolderName}/components/lists/listnav"
                                    backPage="/content/${contentFolderName}-showcase/en/component/media/media-gallery-assetviewer/"
                                    listFrom="collection"/>
                                <sharebutton
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="${appsFolderName}/components/widgets/addthis/sharebutton"/>
                            </par>
                        </toolbar>
                    </par>
                </contentblock-mobile>
                <form_end
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="foundation/components/form/end"/>
            </par>
        </article>
    </jcr:content>
</jcr:root>
