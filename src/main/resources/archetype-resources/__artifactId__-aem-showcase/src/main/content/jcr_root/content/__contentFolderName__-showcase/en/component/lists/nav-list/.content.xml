#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:tags="[${contentFolderName}-showcase:content-type/page]"
            cq:template="/conf/${confFolderName}-showcase/settings/wcm/templates/twocolumn"
            jcr:primaryType="cq:PageContent"
            jcr:title="Nav List"
            sling:resourceType="${appsFolderName}/components/template/base">
        <aside
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/layout/aside"
                cancelInheritParent="true"
                componentId="aside">
            <par
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/container">
                <text
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/content/text"
                        text="&lt;p>Inherit from Parent&lt;/p>&${symbol_pound}xa;"
                        textIsRich="true"/>
                <navlist
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/lists/navlist"
                        componentId="inheritedListInAside"
                        cancelInheritParent="true"
                        listFrom="children"
                        variant="stacked"
                        parentPage="/content/${contentFolderName}-showcase/en/component/lists/nav-list/pages"/>
            </par>
        </aside>
        <article
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/layout/article">
            <par
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/container">
                <page-details
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/details/page-details"/>
                <contentblockmenu1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblockmenu"
                        componentId="contentblockmenu1"/>
                <contentblock1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock1"
                        hideTitle="false"
                        hideTitleSeparator="true"
                        title="Variant: Default with Fixed List"
                        variant="advsection">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <navlist
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/lists/navlist"
                                cancelInheritParent="true"
                                componentId="navlist1"
                                linkTitlePrefix="Test"
                                listFrom="static"
                                pages="[/content/${contentFolderName}-showcase/en/component/lists/nav-list/pages/page1,/content/${contentFolderName}-showcase/en/component/lists/nav-list/pages/page2]"/>
                    </par>
                </contentblock1>
                <contentblock2
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock2"
                        hideTitle="false"
                        hideTitleSeparator="true"
                        title="Variant: Simple with Fixed List"
                        variant="advsection">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <navlist
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/lists/navlist"
                                cancelInheritParent="true"
                                componentId="navlist2"
                                linkTitlePrefix="Go to"
                                listFrom="static"
                                pages="[/content/${contentFolderName}-showcase/en/component/lists/nav-list/pages/page1,/content/${contentFolderName}-showcase/en/component/lists/nav-list/pages/page2]"
                                variant="simple"/>
                    </par>
                </contentblock2>
                <contentblock3
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock3"
                        hideTitle="false"
                        hideTitleSeparator="true"
                        title="Variant: Stacked with Fixed List"
                        variant="advsection">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <navlist
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/lists/navlist"
                                cancelInheritParent="true"
                                componentId="navlist3"
                                linkTitlePrefix="Test"
                                listFrom="static"
                                pages="[/content/${contentFolderName}-showcase/en/component/lists/nav-list,/content/${contentFolderName}-showcase/en/component/lists/nav-list/pages/page1,/content/${contentFolderName}-showcase/en/component/lists/nav-list/pages/page2]"
                                variant="stacked"/>
                    </par>
                </contentblock3>
                <contentblock4
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock4"
                        hideTitle="false"
                        hideTitleSeparator="true"
                        title="Variant: Simple with Children List"
                        variant="advsection">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <navlist
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/lists/navlist"
                                cancelInheritParent="true"
                                componentId="navlist4"
                                linkTitlePrefix="Test"
                                listFrom="children"
                                parentPage="/content/${contentFolderName}-showcase/en/component/lists/nav-list/pages"
                                variant="simple"/>
                    </par>
                </contentblock4>
                <contentblock5
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/contentblock"
                        componentId="contentblock5"
                        hideTitle="false"
                        hideTitleSeparator="true"
                        title="Variant: Stacked with Children List"
                        variant="advsection">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/layout/container">
                        <navlist
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="${appsFolderName}/components/lists/navlist"
                                cancelInheritParent="true"
                                componentId="navlist5"
                                linkTitlePrefix="Test"
                                listFrom="children"
                                parentPage="/content/${contentFolderName}-showcase/en/component/lists/nav-list/pages"
                                variant="stacked"/>
                    </par>
                </contentblock5>
            </par>
        </article>
    </jcr:content>
</jcr:root>
