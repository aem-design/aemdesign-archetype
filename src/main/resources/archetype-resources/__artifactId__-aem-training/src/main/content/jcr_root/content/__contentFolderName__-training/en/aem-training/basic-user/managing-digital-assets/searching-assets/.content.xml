#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[${contentFolderName}:content-type/page]"
        cq:template="/conf/${confFolderName}-training/settings/wcm/templates/twocolumn"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:lockable,mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Searching Assets"
        sling:resourceType="${appsFolderName}/components/template/column/two">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="/apps/${appsFolderName}/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/details/page-details"/>
                <pagelist
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/lists/pagelist"
                    componentTheme="[]"
                    listFrom="children"
                    listType="list"
                    parentPage="/content/${contentFolderName}-training/en/aem-training/basic-user/managing-digital-assets"
                    printStructure="{Boolean}true"
                    tagsMatch="any"/>
                <contentblock
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentTheme="[]"
                    hideTitle="true"
                    hideTitleSeparator="true">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <text_1821146571
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            componentTheme="[]"
                            text="&lt;div>Use the Search panel to search for assets, tags, and metadata. You can search parts of a string using the wildcard asterisk.&lt;/div>&${symbol_pound}xa;&lt;ul>&${symbol_pound}xa;&lt;li>Navigate to &lt;b>Asset Console&lt;/b>. Select &lt;b>Search&lt;/b>.&lt;img src=&quot;/content/dam/${contentFolderName}-training/Managing_Assets-Search_Button.png&quot;>&lt;/li>&${symbol_pound}xa;&lt;li>Fill in the search criteria and click Return button on the keyboard to retrieve the search result.&lt;/li>&${symbol_pound}xa;&lt;/ul>&${symbol_pound}xa;"
                            textIsRich="true"/>
                        <image_1902439303
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/media/image"
                            fileReference="/content/dam/${contentFolderName}-training/Managing_Assets-Search.png"
                            imageOption="fixedImageGenerated"
                            imageRotate="0"
                            width="500"/>
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            text="&lt;p>&amp;nbsp;&lt;/p>&${symbol_pound}xa;&lt;p>&amp;nbsp;&lt;/p>&${symbol_pound}xa;&lt;p>&amp;nbsp;&lt;/p>&${symbol_pound}xa;"
                            textIsRich="true"/>
                    </par>
                </contentblock>
            </par>
        </article>
    </jcr:content>
</jcr:root>
