#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[${contentFolderName}:content-type/page]"
        cq:template="/conf/${confFolderName}-training/settings/wcm/templates/twocolumn"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:lockable,mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Completing a Participant Step"
        sling:resourceType="${appsFolderName}/components/template/column/two">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="/apps/${appsFolderName}/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/details/page-details"/>
                <pagelist
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/lists/pagelist"
                    componentTheme="[]"
                    listFrom="children"
                    listType="list"
                    parentPage="/content/training/en/aem-training/intermediate-user/using-workflows"
                    printStructure="{Boolean}true"
                    tagsMatch="any"/>
                <contentblock
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentTheme="[]"
                    hideTitle="true"
                    hideTitleSeparator="true">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&${symbol_pound}xa;&lt;li>When you are assigned a work item various alerts and/or notifications appear. Your notification indicator (toolbar) will be incremented:&lt;b>&lt;/b>&lt;/li>&${symbol_pound}xa;&lt;/ul>&${symbol_pound}xa;"
                            textIsRich="true"/>
                        <image
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/media/image"
                            fileReference="/content/dam/${contentFolderName}-training/Using_a_Workflow-Notification.png"
                            imageOption="fixedImageGenerated"
                            imageRotate="0"
                            width="200"/>
                        <text_1378668245
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&${symbol_pound}xa;&lt;li>Select &lt;b>View all&lt;/b>.&lt;/li>&${symbol_pound}xa;&lt;li>The item will be listed in your Notification Inbox:&lt;/li>&${symbol_pound}xa;&lt;/ul>&${symbol_pound}xa;"
                            textIsRich="true"/>
                        <image_790871690
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/media/image"
                            fileReference="/content/dam/${contentFolderName}-training/Using_a_Workflow-Notification_List.png"
                            imageOption="fixedImageGenerated"
                            imageRotate="0"
                            width="500"/>
                        <text_791873067
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&${symbol_pound}xa;&lt;li>After you have taken the action indicated that you can complete the work item, thus allowing the workflow to continue.&lt;/li>&${symbol_pound}xa;&lt;li>Activate the click box next to the item you want to take action on.&lt;/li>&${symbol_pound}xa;&lt;li>Use the &lt;b>Complete&lt;/b> icon:&lt;br>&${symbol_pound}xa;&lt;img src=&quot;/content/dam/${contentFolderName}-training/Using_a_Workflow-Complete_Button.png&quot;>&lt;/li>&${symbol_pound}xa;&lt;/ul>&${symbol_pound}xa;"
                            textIsRich="true"/>
                        <text_919789044
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&${symbol_pound}xa;&lt;li>The &lt;b>Complete Work Item&lt;/b> page will open.&lt;/li>&${symbol_pound}xa;&lt;li>Select the &lt;b>Next Step&lt;/b> and add comments in&amp;nbsp;&lt;b>Comment&lt;/b>&amp;nbsp;field if needed. Use &lt;b>Complete&lt;/b> to complete the step (or the &lt;b>Back&lt;/b> icon to abort the action):&lt;/li>&${symbol_pound}xa;&lt;/ul>&${symbol_pound}xa;"
                            textIsRich="true"/>
                        <image_1370219956
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/media/image"
                            fileReference="/content/dam/${contentFolderName}-training/Using_a_Workflow-Complete_the_Workflow_Step.png"
                            imageOption="fixedImageGenerated"
                            imageRotate="0"
                            width="500"/>
                        <text_1100312868
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&${symbol_pound}xa;&lt;li>Use the Complete icon:&lt;br>&${symbol_pound}xa;&lt;img src=&quot;/content/dam/${contentFolderName}-training/Using_a_Workflow-Complete_Button.png&quot;>&lt;/li>&${symbol_pound}xa;&lt;/ul>&${symbol_pound}xa;"
                            textIsRich="true"/>
                        <text_405769885
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            componentTheme="[]"
                            text="&lt;p>Note: On this example, “Create Version” is the next step. After this step is completed, a new version of the page is created. You can examine the result by navigating to the page and select &lt;b>Timeline&lt;/b>. A new version would be created.&lt;/p>&${symbol_pound}xa;"
                            textIsRich="true"/>
                        <image_1892275942
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/media/image"
                            fileReference="/content/dam/${contentFolderName}-training/Using_a_Workflow-Create_a_version_in_Timeline.png"
                            imageOption="fixedImageGenerated"
                            imageRotate="0"
                            width="200"/>
                        <text_1833817615
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&${symbol_pound}xa;&lt;li>Follow and take actions on the rest of the steps until the workflow is completed.&lt;/li>&${symbol_pound}xa;&lt;/ul>&${symbol_pound}xa;&lt;p>&amp;nbsp;&lt;/p>&${symbol_pound}xa;&lt;p>&amp;nbsp;&lt;/p>&${symbol_pound}xa;"
                            textIsRich="true"/>
                    </par>
                </contentblock>
            </par>
        </article>
    </jcr:content>
</jcr:root>
