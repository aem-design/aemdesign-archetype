#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[${contentFolderName}:content-type/page]"
        cq:template="/conf/${confFolderName}-training/settings/wcm/templates/twocolumn"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:lockable,mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Moving Tags"
        sling:resourceType="${appsFolderName}/components/template/column/two">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="/apps/${appsFolderName}/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/details/page-details"/>
                <pagelist
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/lists/pagelist"
                    componentTheme="[]"
                    listFrom="children"
                    listType="list"
                    parentPage="/content/${contentFolderName}-training/en/aem-training/advanced-user/managing-tags"
                    printStructure="{Boolean}true"
                    tagsMatch="any"/>
                <contentblock
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentTheme="[]"
                    hideTitle="true"
                    hideTitleSeparator="true">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <image
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/media/image"
                            fileReference="/content/dam/${contentFolderName}-training/Managing_Tags-Move_Tag_Button.png"
                            imageOption="asIs"
                            imageRotate="0"
                            width="200"/>
                        <text_1267610963
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            componentTheme="[]"
                            text="&lt;div>When a namespace or other tag is selected, selecting the &lt;b>Move&lt;/b> icon will allow Tags Administrators and Developers to clean up the taxonomy by&amp;nbsp;moving the tag to a new location or renaming it. &amp;nbsp;When the selected tag is a container tag, moving the tag will move all child tags as well.&lt;/div>&${symbol_pound}xa;"
                            textIsRich="true"/>
                        <image_1699991257
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/media/image"
                            fileReference="/content/dam/${contentFolderName}-training/Managing_Tags-Move_Tag.png"
                            imageOption="fixedImageGenerated"
                            imageRotate="0"
                            width="500"/>
                        <text_1933801991
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&${symbol_pound}xa;&lt;li>Path – (readonly) The current path to the selected tag.&lt;/li>&${symbol_pound}xa;&lt;li>Move to – Browse to the new path under which to move the tag.&lt;/li>&${symbol_pound}xa;&lt;li>Rename to – Initially displays the current name of the tag. A new name may be entered.&lt;/li>&${symbol_pound}xa;&lt;li>Select &lt;b>Save&lt;/b>.&lt;/li>&${symbol_pound}xa;&lt;/ul>&${symbol_pound}xa;&lt;p>&amp;nbsp;&lt;/p>&${symbol_pound}xa;&lt;p>&amp;nbsp;&lt;/p>&${symbol_pound}xa;"
                            textIsRich="true"/>
                    </par>
                </contentblock>
            </par>
        </article>
    </jcr:content>
</jcr:root>
