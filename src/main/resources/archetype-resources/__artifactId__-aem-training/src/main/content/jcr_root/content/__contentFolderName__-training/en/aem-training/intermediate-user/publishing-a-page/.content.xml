<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[${contentFolderName}:content-type/page]"
        cq:template="/conf/${confFolderName}-training/settings/wcm/templates/twocolumn"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:lockable,mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Publishing a Page"
        sling:resourceType="${appsFolderName}/components/template/column/two">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="/apps/${appsFolderName}/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appsFolderName}/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/details/page-details"/>
                <pagelist
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/lists/pagelist"
                    componentTheme="[]"
                    listFrom="children"
                    listType="list"
                    parentPage="/content/${contentFolderName}-training/en/aem-training/intermediate-user/publishing-a-page"
                    printStructure="{Boolean}true"
                    tagsMatch="any"/>
                <contentblock
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/layout/contentblock"
                    componentTheme="[]"
                    hideTitle="true"
                    hideTitleSeparator="true">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appsFolderName}/components/layout/container">
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="${appsFolderName}/components/content/text"
                            componentTheme="[]"
                            text="&lt;p>This section covers the actions involved in publishing and unpublishing your pages using Adobe Experience Manager (AEM).&lt;/p>&${symbol_pound}xa;&lt;p>Publishing means that content you have created on the author environment will be made available on your public website (your publish environment).&lt;/p>&${symbol_pound}xa;&lt;p>&amp;nbsp;&lt;/p>&${symbol_pound}xa;&lt;p>&amp;nbsp;&lt;/p>&${symbol_pound}xa;&lt;p>&lt;b>Exercise&lt;/b>&lt;/p>&${symbol_pound}xa;"
                            textIsRich="true"/>
                    </par>
                </contentblock>
                <pagelist_58360567
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/lists/pagelist"
                    componentTheme="[]"
                    listFrom="static"
                    listType="list"
                    pages="/content/${contentFolderName}-training/en/aem-training/intermediate-user/publishing-a-page/exercise-publishing-a-page"
                    printStructure="{Boolean}true"
                    tagsMatch="any"/>
                <text
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appsFolderName}/components/content/text"
                    componentTheme="[]"
                    text="&lt;p>&amp;nbsp;&lt;/p>&${symbol_pound}xa;&lt;p>&amp;nbsp;&lt;/p>&${symbol_pound}xa;&lt;p>&amp;nbsp;&lt;/p>&${symbol_pound}xa;&lt;p>&amp;nbsp;&lt;/p>&${symbol_pound}xa;"
                    textIsRich="true"/>
